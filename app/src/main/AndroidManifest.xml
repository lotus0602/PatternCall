<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.n.patterncall" >

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".app.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".app.PatternLockActivity" >
        </activity>
        <activity android:name=".app.SetPatternActivity" >
        </activity>
        <activity android:name=".app.ConfirmPatternActivity" >
        </activity>
        <activity android:name=".app.PatternCallActivity" >
        </activity>
        <activity android:name=".app.CallerActivity" >
        </activity>
    </application>

</manifest>
